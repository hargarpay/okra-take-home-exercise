{
    "compilerOptions": {
      "module": "commonjs",
      "esModuleInterop": true,
      "target": "es6",
      "moduleResolution": "node",
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "outDir": "dist",
      "skipLibCheck": true
    },
    "lib": ["es2015"],
    "include": ["src/**/*"]
  }